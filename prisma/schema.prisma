datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Job {
  id         String    @id @default(uuid())
  status     String    @default("pending")
  fileName   String?
  filePath   String?
  preview    Json?
  fullResult Json?
  isPaid     Boolean   @default(false)
  paidAt     DateTime?
  createdAt  DateTime  @default(now())
  updatedAt  DateTime  @updatedAt
}